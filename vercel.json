{
  "buildCommand": "npm run build",
  "outputDirectory": "build/client",
  "functions": {
    "api/*.cjs": {
      "runtime": "@vercel/node@20"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.cjs"
    }
  ]
}
